{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cla-blocks/sequence",
	"version": "0.1.0",
	"title": "Sequence",
	"category": "cla",
	"icon": "smiley",
	"description": "A responsive sequence (timeline) block container for Sequence Item blocks.",
	"example": {
		"innerBlocks": [
			{
				"name": "cla-blocks/sequence-item"
			},
			{
				"name": "cla-blocks/sequence-item"
			},
			{
				"name": "cla-blocks/sequence-item"
			}
		],
		"viewportWidth": 400
	},
	"supports": {
		"align": ["wide", "full"],
		"html": false
	},
	"textdomain": "cla-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"allowedBlocks": [
		"cla-blocks/sequence-item"
	],
	"attributes": {}
}