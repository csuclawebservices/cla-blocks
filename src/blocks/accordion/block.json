{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cla-blocks/accordion",
	"version": "0.1.0",
	"title": "Accordion",
	"category": "cla",
	"icon": "smiley",
	"description": "A responsive accordion block.",
	"example": {
		"innerBlocks": [
			{
				"name": "cla-blocks/accordion-item",
				"attributes": {
					"title": "Accordion Item One"
				}
			},
			{
				"name": "cla-blocks/accordion-item",
				"attributes": {
					"title": "Accordion Item Two"
				}
			},
			{
				"name": "cla-blocks/accordion-item",
				"attributes": {
					"title": "Accordion Item Three"
				}
			}
		],
		"viewportWidth": 768
	},
	"supports": {
		"align": ["wide", "full"],
		"color": {
			"background": true,
			"button": false,
			"heading": true,
			"link": true,
			"text": true
		},
		"html": false
	},
	"textdomain": "cla-blocks",
	"editorScript": "file:./index.js",
	"script": ["cla-blocks-accordion"],
	"viewScript": ["file:./view.js"],
	"style": "file:./style-index.css",
	"editorStyle": "file:./index.css",
	"allowedBlocks": [
		"cla-blocks/accordion-item"
	],
	"attributes": {}
}