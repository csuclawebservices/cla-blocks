{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cla-blocks/image-group",
  "version": "0.1.0",
  "title": "Image Group",
  "category": "theme",
  "icon": "smiley",
  "description": "A responsive image group block container for Image Group blocks.",
  "example": {},
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "textdomain": "cla-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "allowedBlocks": [
    "cla-blocks/image-group-item"
  ],
  "attributes": {
    "maxColumns": {
      "type": "integer",
      "default": 3
    }
  }
}